{
  "smithyLsp.rootPath": "${workspaceRoot}/demo/api/model",
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true,
    "**/node_modules/**/*": true,
    "**/.cache/**/*": true,
    "**/build/**/*": true
  },
  "prettier.prettierPath": "node_modules/prettier/index.js",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "gradle.nestedProjects": true,
  "python.analysis.extraPaths": [
    "${workspaceFolder}/demo/api/generated/runtime/python"
  ],
  "prettier.trailingComma": "all",
  "prettier.singleQuote": true,
  "prettier.printWidth": 120,
  "typescript.preferences.quoteStyle": "single",
  "javascript.preferences.quoteStyle": "single",
  "eslint.nodePath": "node_modules/eslint/lib",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.workingDirectories": [
    "./",
    "./demo/api/interceptors/typescript",
    "./demo/corpus/logic",
    "./demo/infra",
    "./demo/sample-dataset",
    "./demo/website",
    "./packages/galileo-cdk",
    "./packages/galileo-cli",
    "./packages/galileo-sdk"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": false
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
