      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.dev.json"
      }
    }
  },
  "private": true,
  "files": [
    "bin",
    "oclif.manifest.json"
  ],
  "oclif": {
    "bin": "galileo-cli-experimental",
    "dirname": "galileo-cli-dir",
    "commands": "src/commands",
    "hooks": {
      "init": [
        "src/hooks/init"
      ],
      "prerun": [],
      "postrun": [],
      "command_not_found": []
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " "
  },
